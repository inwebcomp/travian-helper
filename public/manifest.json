{
  "name": "Travian Helper",
  "description": "Travian tool for simplify game process",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "/favicon.png"
    }
  },
  "icons": {
    "128": "/favicon.png"
  }
}
